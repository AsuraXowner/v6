AsuraX.IconRememberSwitch.MouseButton1Click:Connect(function()
	if RememberMe==false then
	AsuraX.TextRememberSwitch.Text = "ON"
	game:GetService("TweenService"):Create(AsuraX.TextRemember,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{TextColor3 = Color3.fromRGB(177, 177, 177)}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextRememberSwitch,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{TextColor3 = Color3.fromRGB(177, 177, 177)}):Play()
	game:GetService("TweenService"):Create(AsuraX.IconRememberSwitch,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{ImageColor3 = Color3.fromRGB(177, 177, 177)}):Play()
	RememberMe=true
	elseif RememberMe==true then
	AsuraX.TextRememberSwitch.Text = "OFF"
	game:GetService("TweenService"):Create(AsuraX.TextRemember,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{TextColor3 = Color3.fromRGB(74, 79, 99)}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextRememberSwitch,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{TextColor3 = Color3.fromRGB(74, 79, 99)}):Play()
	game:GetService("TweenService"):Create(AsuraX.IconRememberSwitch,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{ImageColor3 = Color3.fromRGB(42, 41, 57)}):Play()
	RememberMe=false
	end
end)
