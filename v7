			AsuraX.ExecutorFrame.ListGui["Editor#4"].RemoveIcon.ImageTransparency = 0.2
		end
		if Tab5Active==true then
			AsuraX.ExecutorFrame.ListGui["Editor#5"].BackgroundTransparency = 0
			AsuraX.ExecutorFrame.ListGui["Editor#5"].TextButton.TextTransparency = 0
			AsuraX.ExecutorFrame.ListGui["Editor#5"].RemoveIcon.ImageTransparency = 0.2
		end		
		if Tab6Active==true then
			AsuraX.ExecutorFrame.ListGui["Editor#6"].BackgroundTransparency = 0
			AsuraX.ExecutorFrame.ListGui["Editor#6"].TextButton.TextTransparency = 0
			AsuraX.ExecutorFrame.ListGui["Editor#6"].RemoveIcon.ImageTransparency = 0.2
		end
