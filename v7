		warn("Failure probably script patched or bug with executor")
	end
end

MainGui.Icons.CloseButton.MouseButton1Click:Connect(function()
	AsuraX.MainBackground2.Parent:Destroy()
	AsuraX.LoginGui:Destroy()
end)
AsuraX.CloseButton2.MouseButton1Click:Connect(function()
	AsuraX.MainBackground2.Parent:Destroy()
	AsuraX.LoginGui:Destroy()
end)

MainGui.Icons.HideButton.MouseButton1Click:Connect(function()
	AsuraX.ExecutorFrame.DownGui.ExecuteFrame.BackgroundTransparency = 1
	AsuraX.ExecutorFrame.DownGui.ClearFrame.BackgroundTransparency = 1
	AsuraX.ExecutorFrame.DownGui.AttachFrame.BackgroundTransparency = 1
	AsuraX.ExecutorFrame.DownGui.ExecuteFrame.ImageButton.ImageTransparency = 1
	AsuraX.ExecutorFrame.DownGui.AttachFrame.ImageButton.ImageTransparency = 1
	AsuraX.ExecutorFrame.DownGui.ClearFrame.ImageButton.ImageTransparency = 1
	AsuraX.ExecutorFrame.DownGui.AttachFrame.UIStroke.Transparency = 1
	AsuraX.ExecutorFrame.DownGui.ClearFrame.UIStroke.Transparency = 1
	AsuraX.ExecutorFrame.DownGui.ExecuteFrame.UIStroke.Transparency = 1
	
	AsuraX.ExecutorFrame.BackgroundTransparency = 1
	AsuraX.ExecutorFrame.ExecutorText.BackgroundTransparency = 1
	AsuraX.ExecutorFrame.ExecutorText.ScrollingFrame.TextBox.TextTransparency = 1
	AsuraX.ExecutorFrame.ListGui.ADDTabFrame.ADDTab.ImageTransparency = 1
	AsuraX.ExecutorFrame.ListGui["Editor#1"].BackgroundTransparency = 1
	AsuraX.ExecutorFrame.ListGui["Editor#1"].TextButton.TextTransparency = 1
	AsuraX.ExecutorFrame.ListGui["Editor#1"].RemoveIcon.ImageTransparency = 1
	if Tab2Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#2"].BackgroundTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#2"].TextButton.TextTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#2"].RemoveIcon.ImageTransparency = 1
	end
	if Tab3Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#3"].BackgroundTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#3"].TextButton.TextTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#3"].RemoveIcon.ImageTransparency = 1
	end
	if Tab4Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#4"].BackgroundTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#4"].TextButton.TextTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#4"].RemoveIcon.ImageTransparency = 1
	end
	if Tab5Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#5"].BackgroundTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#5"].TextButton.TextTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#5"].RemoveIcon.ImageTransparency = 1
	end		
	if Tab6Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#6"].BackgroundTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#6"].TextButton.TextTransparency = 1
		AsuraX.ExecutorFrame.ListGui["Editor#6"].RemoveIcon.ImageTransparency = 1
	end
	AsuraX.MainBackground2.Frames.ScriptFrame.Visible = false
	game:GetService("TweenService"):Create(AsuraX.Button1Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button1Background.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button1Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background.Button2,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background.Button3,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.Button4,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.MainBackground2.ImageLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	AsuraX.ScrollingFrame.Visible=false
	AsuraX.ScrollingFrame_2.Visible=false
	AsuraX.MainBackground2:TweenSize(
		UDim2.new(0, 641,0, 25),
		"Out",
		"Quad",
		0.3,
		false
	)
	game:GetService("TweenService"):Create(MainGui.Icons.CloseButton,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(MainGui.Icons.HideButton,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	wait(0.3)
	game:GetService("TweenService"):Create(AsuraX.MainBackground2,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	wait(0.2)
	AsuraX.MainBackground2.Visible = false
	GuiHidden=true
end)

