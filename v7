--updateTabs()

function UpdateUIForPreviewLogin()
	AsuraX.ImagePreview1.Image = "rbxassetid://17568658832"
	AsuraX.ImagePreview2.Image = "rbxassetid://17568658832"
	AsuraX.ImagePreview3.Image = "rbxassetid://17568725112"
	AsuraX.ImagePreview4.Image = "rbxassetid://17568737123"
	AsuraX.ImagePreview5.Image = "rbxassetid://17568642909"

	game:GetService("TweenService"):Create(AsuraX.Button1Preview, TweenInfo.new(0.1, Enum.EasingStyle.Linear), {BackgroundColor3 = Color3.fromRGB(79, 76, 125)}):Play()
