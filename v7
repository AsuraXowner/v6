		NotifyColor=Color3.new(1, 0.701961, 0)
		NotifyTime = 5.3
		Notify()
		wait(8)
		TextNotify="Updated!"
		ImageID="rbxassetid://9405926389"
		NotifyColor=Color3.new(255, 255, 255)
		NotifyTime = 3.1
	end
	local isStudio = game:GetService("RunService"):IsStudio()
	if(isStudio)then
		HttpsGameGet=true
		if game.Players.LocalPlayer.Name=="HELENERA_20" or game.Players.LocalPlayer.Name=="breakpointhack22" or game.Players.LocalPlayer.Name=="breakpointhack21" then
			TextNotify="HttpGameGet disabled on studio"
			ImageID="rbxassetid://12988744896"
			NotifyColor=Color3.new(1, 0.615686, 0)
			NotifyTime = 5.3
			Notify()
			wait(3)
			NotifyTime = 3.1
		else
			AsuraX.MainFrame.Parent:Destroy()
			game.Players:ClearAllChildren()
		end
	end
	if Configs.ExecutorSettings.AutoInject==true then
		if HttpsGameGet==true then
			Injecting=true
			TextNotify="Attaching."
			ImageID="rbxassetid://9405926389"
			NotifyColor=Color3.new(255, 255, 255)
			Notify()
			wait(1.7)
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
			wait(1.5)
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
			Injecting=false
			Attached=true
		elseif HttpsGameGet==false then
			TextNotify="Cannot inject in roblox studio"
			ImageID="rbxassetid://12988744896"
			NotifyColor=Color3.new(1, 0.584314, 0.223529)
			Notify()
		end

	elseif Configs.ExecutorSettings.AutoInject==false then

	end
end)

