AsuraX.ExecutorFrame.DownGui.AttachFrame.ImageButton.MouseButton1Click:Connect(function()
	if NotPatched==true then
		if Attached==false then
			if Injecting==false then
				if HttpsGameGet==true then
					Injecting=true
					TextNotify="Attaching."
					ImageID="rbxassetid://9405926389"
					NotifyColor=Color3.new(255, 255, 255)
					Notify()
					wait(1.7)
					StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
					wait(1.5)
					StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
					Injecting=false
					Attached=true
				elseif HttpsGameGet==false then
					TextNotify="Cannot inject in studio"
					ImageID="rbxassetid://12988744896"
					NotifyColor=Color3.new(1, 0.584314, 0.223529)
					Notify()
				end
			elseif Injecting==true then
				TextNotify="Already attaching."
				ImageID="rbxassetid://9405926389"
				NotifyColor=Color3.new(255, 255, 255)
				Notify()
			end
		elseif Attached==true then
			if Injecting==false then
				TextNotify="Already attached."
				ImageID="rbxassetid://9405926389"
				NotifyColor=Color3.new(255, 255, 255)
				Notify()
			end
		end
	elseif NotPatched==false then
		TextNotify="Roblox version mismatch probably executor patched"
		ImageID="rbxassetid://12988744896"
		NotifyColor=Color3.new(1, 0.313725, 0.439216)
		Notify()
	end
end)

--[[local function updateTabs()
	if Tab2Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#2"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#2"]:Destroy()
			Tab2Active = false
			ExecutorText.Visible = true
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 1
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab3Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#3"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#3"]:Destroy()
			Tab3Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = true
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 2
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab4Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#4"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#4"]:Destroy()
			Tab4Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = true
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 3
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab5Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#5"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#5"]:Destroy()
			Tab5Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = true
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
