			if frameExists("Messages") then
				Instances.Messages.Name="Messages"
			elseif frameExists("Messages1") then
				Instances.Messages1.Name="Messages"
			elseif frameExists("Messages3") then
				Instances.Messages3.Name="Messages"
			elseif frameExists("Messages4") then
				Instances.Messages4.Name="Messages"
			elseif frameExists("Messages5") then
				Instances.Messages5.Name="Messages"
			elseif frameExists("Messages6") then
				Instances.Messages6.Name="Messages"
			end
		elseif frameExists("Messages6") then
			Message5=true
			Sound3:play()
			Instances.Messages6.Name="Messages5"
			Instances.Messages5:Clone().Parent = AsuraX.MainBackground2.Notify.NotifyList
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundColor3 = NotifyColor
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, -255,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			wait(0.4)
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, 0,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTransparency=0
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundTransparency=0
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
			AsuraX.MainBackground2.Notify.NotifyList.Messages5:TweenSize(--Timer
				UDim2.new(0, 0,0, 50),
				"Out",
				"Quad",
				NotifyTime,
				false
			)
			wait(NotifyTime)
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, -255,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)
			wait(0.4)
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundLoad:TweenSize(
				UDim2.new(0, 0,0, 62),
				"Out",
				"Quad",
				0.4,
				false
			)

			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.BackgroundTransparency=1
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
			AsuraX.MainBackground2.Notify.NotifyList.Messages5.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
			wait(0.7)
			AsuraX.MainBackground2.Notify.NotifyList.Messages5:Destroy()
			Message5=false
			if frameExists("Messages") then
				Instances.Messages.Name="Messages"
			elseif frameExists("Messages1") then
				Instances.Messages1.Name="Messages"
			elseif frameExists("Messages3") then
				Instances.Messages3.Name="Messages"
			elseif frameExists("Messages4") then
				Instances.Messages4.Name="Messages"
			elseif frameExists("Messages5") then
				Instances.Messages5.Name="Messages"
			elseif frameExists("Messages6") then
				Instances.Messages6.Name="Messages"
			end
		end
	end
end

local player = game.Players.LocalPlayer


