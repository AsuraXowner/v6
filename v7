AsuraX.Button1Background.Button1.MouseButton1Click:Connect(function()
	if GuiHidden == true then
	ShowHidden()
	game:GetService("TweenService"):Create(AsuraX.Button1Background.Button1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageTransparency = 0.46}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button1Background.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Transparency = 0}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background.Button2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageTransparency = 0.66}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background.Button3,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageTransparency = 0.66}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.Button4,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageTransparency = 0.66}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.UIStroke,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{Transparency = 0}):Play()
	end
	GuiHidden = false
	game:GetService("TweenService"):Create(AsuraX.Button2Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background.Button2,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 0.66}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button2Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background.Button3,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 0.66}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button3Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.Button4,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 0.66}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	--hide
	game:GetService("TweenService"):Create(AsuraX.Button1Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button1Background.Button1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 0.46}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button1Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 0}):Play()
	HideName="PanelFrame"
	Show()
	Button1List=true
	Button2List=false
	Button3List=false
	Button4List=false
end)

