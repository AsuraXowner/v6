--[[local textBox = ExecutorText -- Assuming the TextBox is a child of the script's parent
local previousLineCount = 0

local function countLines(text)
	local _, lineCount = text:gsub('\n', '\n')
	return lineCount + 1
end

local function updateLineCount()
	local text = textBox.Text
	local lineCount = countLines(text)
	if lineCount ~= previousLineCount then
		previousLineCount = lineCount
		print("Line Count:", lineCount)
		AsuraX.TextLabelTextbox.Text= ('\n'..lineCount)
	end
end

textBox:GetPropertyChangedSignal("Text"):Connect(updateLineCount)--]]

local textBox = ExecutorText
local textBox2 = ExecutorText2
local textBox3 = ExecutorText3
local textBox4 = ExecutorText4
local textBox5 = ExecutorText5
local textBox6 = ExecutorText6

local previousLineCount = 0
local previousLineCount2 = 0
local previousLineCount3 = 0
local previousLineCount4 = 0
local previousLineCount5 = 0
local previousLineCount6 = 0

local function countLines(text)
	local _, lineCount = text:gsub('\n', '\n')
	return lineCount + 1
end
local function countLines2(text2)
	local _, lineCount2 = text2:gsub('\n', '\n')
	return lineCount2 + 1
end
local function countLines3(text3)
	local _, lineCount3 = text3:gsub('\n', '\n')
	return lineCount3 + 1
end
local function countLines4(text4)
	local _, lineCount4 = text4:gsub('\n', '\n')
	return lineCount4 + 1
end
local function countLines5(text5)
	local _, lineCount5 = text5:gsub('\n', '\n')
	return lineCount5 + 1
end
local function countLines6(text6)
	local _, lineCount6 = text6:gsub('\n', '\n')
	return lineCount6 + 1
end

local function updateLineCount()
	if TabOpen==1 then
	local text = textBox.Text
	local lineCount = countLines(text)
	if lineCount ~= previousLineCount then
		previousLineCount = lineCount
		--print("Line Count:", lineCount)
		-- Construct line numbers string
		local lineNumbers = ""
		for i = 1, lineCount do
			lineNumbers = lineNumbers .. i .. "\n"
		end
		-- Update the Text property of AsuraX TextBox
		AsuraX.TextLabelTextbox.Text = lineNumbers
		end
	elseif TabOpen==2 then
		local text2 = textBox2.Text
		local lineCount2 = countLines2(text2)
		if lineCount2 ~= previousLineCount2 then
			previousLineCount2 = lineCount2
			local lineNumbers2 = ""
			for i = 1, lineCount2 do
				lineNumbers2 = lineNumbers2 .. i .. "\n"
			end
			AsuraX.TextLabelTextbox.Text = lineNumbers2
		end
	elseif TabOpen==3 then
		local text3 = textBox3.Text
		local lineCount3 = countLines3(text3)
		if lineCount3 ~= previousLineCount3 then
			previousLineCount3 = lineCount3
			local lineNumbers3 = ""
			for i = 1, lineCount3 do
				lineNumbers3 = lineNumbers3 .. i .. "\n"
			end
			AsuraX.TextLabelTextbox.Text = lineNumbers3
		end
	elseif TabOpen==4 then
		local text4 = textBox4.Text
		local lineCount4 = countLines4(text4)
		if lineCount4 ~= previousLineCount4 then
			previousLineCount4 = lineCount4
			local lineNumbers4 = ""
			for i = 1, lineCount4 do
				lineNumbers4 = lineNumbers4 .. i .. "\n"
			end
			AsuraX.TextLabelTextbox.Text = lineNumbers4
		end
	elseif TabOpen==5 then
		local text5 = textBox5.Text
		local lineCount5 = countLines5(text5)
		if lineCount5 ~= previousLineCount5 then
			previousLineCount5 = lineCount5
			local lineNumbers5 = ""
			for i = 1, lineCount5 do
				lineNumbers5 = lineNumbers5 .. i .. "\n"
			end
			AsuraX.TextLabelTextbox.Text = lineNumbers5
		end
	elseif TabOpen==6 then
		local text6 = textBox6.Text
		local lineCount6 = countLines6(text6)
		if lineCount6 ~= previousLineCount6 then
			previousLineCount6 = lineCount6
			local lineNumbers6 = ""
			for i = 1, lineCount6 do
				lineNumbers6 = lineNumbers6 .. i .. "\n"
			end
			AsuraX.TextLabelTextbox.Text = lineNumbers6
		end
		
	end
end

textBox:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
textBox2:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
textBox3:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
textBox4:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
textBox5:GetPropertyChangedSignal("Text"):Connect(updateLineCount)
textBox6:GetPropertyChangedSignal("Text"):Connect(updateLineCount)

textBox.Changed:Connect(function(property)
	if property == "Text" then
		updateLineCount()
	end
end)
textBox2.Changed:Connect(function(property)
	if property == "Text" then
		updateLineCount()
	end
end)
textBox3.Changed:Connect(function(property)
	if property == "Text" then
		updateLineCount()
	end
end)
textBox4.Changed:Connect(function(property)
	if property == "Text" then
		updateLineCount()
	end
end)
textBox5.Changed:Connect(function(property)
	if property == "Text" then
		updateLineCount()
	end
end)
textBox6.Changed:Connect(function(property)
	if property == "Text" then
		updateLineCount()
	end
end)


