	while true do
		wait(0.5)
		--local function setupEventHandlers()
	--if AsuraX.MainBackground2.Frames and AsuraX.MainBackground2.Frames:FindFirstChild("ExecutorFrame") and AsuraX.ExecutorFrame:FindFirstChild("ListGui") and AsuraX.ExecutorFrame.ListGui:FindFirstChild("ADDTabFrame") and AsuraX.ExecutorFrame.ListGui.ADDTabFrame:FindFirstChild("ADDTab") then
		AsuraX.ExecutorFrame.ListGui.ADDTabFrame.ADDTab.MouseButton1Click:Connect(function()
			TabAdd()
			--TextNotify="Tab system disabled"
			--ImageID="rbxassetid://9405926389"
			--NotifyColor=Color3.new(1, 0.466667, 0)
			--Notify()
	end)
		--print("Event handlers set up successfully.")
	--else
		--warn("One or more elements in the path AsuraX.ExecutorFrame.ListGui.ADDTabFrame.ADDTab are not found")
	--end
	if Tab2Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#2"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#2"]:Destroy()
			Tab2Active = false
			ExecutorText.Visible = true
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 1
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab3Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#3"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#3"]:Destroy()
			Tab3Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = true
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 2
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab4Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#4"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#4"]:Destroy()
			Tab4Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = true
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 3
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab5Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#5"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#5"]:Destroy()
			Tab5Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = true
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			TabOpen = 4
			TabNumber = TabNumber - 1
			updateLineCount()
		end)
	end

	if Tab6Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#6"].RemoveIcon.MouseButton1Click:Connect(function()
			AsuraX.ExecutorFrame.ListGui["Editor#6"]:Destroy()
			Tab6Active = false
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = true
			ExecutorText6.Visible = false
			TabOpen = 5
