local playerID = game.Players.LocalPlayer.Name

local isAuthorized = false
local keyFound = false

local function handleLogin()
	local username = AsuraX.TextBoxLogin1.Text
	local keys = AsuraX.TextBoxLogin2.Text
	local password = AsuraX.TextBoxPassword3.Text
	for _, name in ipairs(UsersID) do
		if name == playerID then
			isAuthorized = true
			break
		end
	end
	for _, key in ipairs(Keys) do
		if key == keys then
			keyFound = true
			break
		end
	end
	if ActivateName=="Paid" then
	if isAuthorized then
	if users[username] and users[username] == password then
			LoadLogin()
			PreviewLogin=false
		else
			AsuraX.TextIncorrect.Text="Login incorrect"
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.1}):Play()
			wait(2)
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=1}):Play()
		end
	else
		AsuraX.TextIncorrect.Text="password incorrect"
		game:GetService("TweenService"):Create(AsuraX.TextIncorrect,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=0.1}):Play()
		wait(2)
		game:GetService("TweenService"):Create(AsuraX.TextIncorrect,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{TextTransparency=1}):Play()
		end
	elseif ActivateName=="Key" then 
		if keyFound then
			AsuraX.TextIncorrect.Text = "Failed to launch executor error"
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {TextTransparency = 0.1}):Play()
			wait(2)
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {TextTransparency = 1}):Play()
			--LoadLogin()
			--PreviewLogin = false
		else
			AsuraX.TextIncorrect.Text = "Key incorrect"
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {TextTransparency = 0.1}):Play()
			wait(2)
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {TextTransparency = 1}):Play()
		end
	elseif ActivateName=="Free" then 
			AsuraX.TextIncorrect.Text = "Failed to launch executor error"
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {TextTransparency = 0.1}):Play()
			wait(2)
			game:GetService("TweenService"):Create(AsuraX.TextIncorrect, TweenInfo.new(0.2, Enum.EasingStyle.Linear), {TextTransparency = 1}):Play()
	end
	
end
AsuraX.TextLaunch.MouseButton1Click:Connect(handleLogin)

function ShowHidden()
	AsuraX.MainBackground2.Visible = true
	AsuraX.ScrollingFrame.Visible=true
	AsuraX.ScrollingFrame_2.Visible=true
	game:GetService("TweenService"):Create(AsuraX.MainBackground2.Buttons.SidePanel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(AsuraX.MainBackground2.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 0}):Play()
	game:GetService("TweenService"):Create(MainGui.Icons.CloseButton,TweenInfo.new(0.3,Enum.EasingStyle.Linear),{ImageTransparency = 0.4}):Play()
