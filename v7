function LoadLogin()
	PreviewLogin=false
	game:GetService("TweenService"):Create(AsuraX.IconRememberSwitch,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.Logo,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.UIStrokeCustomImage39,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.UIStrokeCustomImage40,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.UIStrokeCustomImage38,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.KeyLogin,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextBoxLogin1,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextAbout56,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextRemember,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextRememberSwitch,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextForgot,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(AsuraX.TextForgot2,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{TextTransparency = 1}):Play()
