	game:GetService("TweenService"):Create(AsuraX.Button4Background,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.Button4,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{ImageTransparency = 0.46}):Play()
	game:GetService("TweenService"):Create(AsuraX.Button4Background.UIStroke,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{Transparency = 0}):Play()
	HideName="SettingsFrame"
	Show()
	Button1List=false
	Button2List=false
	Button3List=false
	Button4List=true
end)

