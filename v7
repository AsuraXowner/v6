		end
end)

	
AsuraX.ExecutorFrame.DownGui.ClearFrame.ImageButton.MouseButton1Click:Connect(function()
		if TabOpen==1 then
		ExecutorText.Text=""
		elseif TabOpen==2 then
		ExecutorText2.Text=""
		elseif TabOpen==3 then
		ExecutorText3.Text=""
		elseif TabOpen==4 then
		ExecutorText4.Text=""
		elseif TabOpen==5 then
		ExecutorText5.Text=""
		elseif TabOpen==6 then
		ExecutorText6.Text=""
		end
	end)
	

    local Gui = AsuraX.MainBackground2
	
	local gui = Gui
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
end)

local function frameExists(frameName)
	return Instances:FindFirstChild(frameName) ~= nil
end



--mhm-mhmhm-hm-hmh-m-mh-mh-mh-mh-m-h-m-hm--AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA que que que so..
function Notify()
	if Message==false then
		Message=true
		Sound3:play()
		if frameExists("Messages") then
			Instances.Messages.Name="Messages"
		elseif frameExists("Messages1") then
			Instances.Messages1.Name="Messages"
		elseif frameExists("Messages3") then
			Instances.Messages3.Name="Messages"
		elseif frameExists("Messages4") then
			Instances.Messages4.Name="Messages"
		elseif frameExists("Messages5") then
			Instances.Messages5.Name="Messages"
		elseif frameExists("Messages6") then
			Instances.Messages6.Name="Messages"
		end
		Instances.Messages:Clone().Parent = AsuraX.MainBackground2.Notify.NotifyList
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundColor3 = NotifyColor
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, -255,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)
		wait(0.4)
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, 0,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTransparency=0
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundTransparency=0
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
		AsuraX.MainBackground2.Notify.NotifyList.Messages:TweenSize(--Timer
			UDim2.new(0, 0,0, 50),
			"Out",
			"Quad",
			NotifyTime,
			false
		)
		wait(NotifyTime)
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, -255,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)
		wait(0.4)
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
			UDim2.new(0, 0,0, 62),
			"Out",
			"Quad",
			0.4,
			false
		)

		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.BackgroundTransparency=1
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
		AsuraX.MainBackground2.Notify.NotifyList.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
		wait(0.7)
		AsuraX.MainBackground2.Notify.NotifyList.Messages:Destroy()
		Message=false
		if frameExists("Messages") then
			Instances.Messages.Name="Messages"
		elseif frameExists("Messages1") then
			Instances.Messages1.Name="Messages"
		elseif frameExists("Messages3") then
			Instances.Messages3.Name="Messages"
