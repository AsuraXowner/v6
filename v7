--panelframebuttons
AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button3Panel.Image3244.MouseButton1Click:Connect(function()
	setclipboard("P4XBw7vQVU")
	TextNotify="Copied to clipboard"
	ImageID="rbxassetid://9405926389"
	NotifyColor=Color3.new(255, 255, 255)
	NotifyTime = 2.3
	Notify()
	wait(3)
	NotifyTime = 3.1
end)
AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button4Panel.Image3247.MouseButton1Click:Connect(function()
	if HttpsGameGet==true then
    game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId)
	game:GetService("TweenService"):Create(AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button4Panel.Image3247,TweenInfo.new(0.6,Enum.EasingStyle.Linear),{Rotation = 360}):Play()
	wait(0.6)
	AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button4Panel.Image3247.Rotation = 0
	TextNotify="Rejoining"
	ImageID="rbxassetid://9405926389"
	NotifyColor=Color3.new(255, 255, 255)
		Notify()
	elseif HttpsGameGet==false then
		game:GetService("TweenService"):Create(AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button4Panel.Image3247,TweenInfo.new(0.4,Enum.EasingStyle.Linear),{Rotation = 360}):Play()
		wait(0.4)
		AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button4Panel.Image3247.Rotation = 0
		TextNotify="Rejoin Failed"
		ImageID="rbxassetid://12988744896"
		NotifyColor=Color3.new(1, 0.25098, 0.262745)
		Notify()
	end
end)
--
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local userId = player.UserId
local imageSize = Enum.ThumbnailSize.Size420x420
local thumbnailType = Enum.ThumbnailType.HeadShot

local function updateProfilePicture()
	local thumbnailUrl = Players:GetUserThumbnailAsync(userId, thumbnailType, imageSize)
	
	AsuraX.MainBackground2.Frames.PanelFrame.Buttons3.Button2Panel.Image3245.Image = thumbnailUrl
end

updateProfilePicture()


