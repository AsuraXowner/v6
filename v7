local GuiService = game:GetService("GuiService")
local TextLabel = AsuraX.MainFrame
local Camera = workspace.CurrentCamera

-- Function to update the TextLabel position
local function updatePosition()
	-- Get the insets from GuiService
	local insetTopLeft = GuiService:GetGuiInset()

	-- Set the position of the TextLabel taking the top inset into account
	TextLabel.Position = UDim2.new(0, 0, 0, insetTopLeft.Y)
end

-- Update the position initially
updatePosition()

-- Connect to the event that fires when the camera's viewport size changes
Camera:GetPropertyChangedSignal("ViewportSize"):Connect(updatePosition)



