	if Configs.Settings.makefolder==true then
		print("✅ makefolder")
	elseif Configs.Settings.makefolder==false then
		print("⛔ makefolder")
	end
	print("✅ appendfile")
	print("✅ isfile")
	print("✅ isfolder")
	print("✅ delfolder")
	print("✅ delfile")
	print("✅ loadfile")
	print("⏺️ dofile")
