			updateLineCount()
		end)
	end

	if Tab1Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#1"].TextButton.MouseButton1Click:Connect(function()
			ExecutorText.Visible = true
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			updateLineCount()
			TabOpen = 1
		end)
	end

	if Tab2Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#2"].TextButton.MouseButton1Click:Connect(function()
			ExecutorText.Visible = false
			ExecutorText2.Visible = true
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			updateLineCount()
			TabOpen = 2
		end)
	end

	if Tab3Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#3"].TextButton.MouseButton1Click:Connect(function()
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = true
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			updateLineCount()
			TabOpen = 3
		end)
	end

	if Tab4Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#4"].TextButton.MouseButton1Click:Connect(function()
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = true
			ExecutorText5.Visible = false
			ExecutorText6.Visible = false
			updateLineCount()
			TabOpen = 4
		end)
	end

	if Tab5Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#5"].TextButton.MouseButton1Click:Connect(function()
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = true
			ExecutorText6.Visible = false
			updateLineCount()
			TabOpen = 5
		end)
	end

	if Tab6Active==true then
		AsuraX.ExecutorFrame.ListGui["Editor#6"].TextButton.MouseButton1Click:Connect(function()
			ExecutorText.Visible = false
			ExecutorText2.Visible = false
			ExecutorText3.Visible = false
			ExecutorText4.Visible = false
			ExecutorText5.Visible = false
			ExecutorText6.Visible = true
			updateLineCount()
			TabOpen = 6
		end)
	end
end
--end
