local function dragUI(frame)
	local dragging2
	local dragStart2
	local startPos2

	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging2 = true
			dragStart2 = input.Position
			startPos2 = AsuraX.BackgroundMain.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging2 = false
				end
			end)
		end
	end)

	AsuraX.BackgroundMain.InputChanged:Connect(function(input)
		if dragging2 and input.UserInputType == Enum.UserInputType.MouseMovement then
			local delta = input.Position - dragStart2
			AsuraX.BackgroundMain.Position = UDim2.new(startPos2.X.Scale, startPos2.X.Offset + delta.X, startPos2.Y.Scale, startPos2.Y.Offset + delta.Y)
		end
	end)
end

--dragUI(AsuraX.BackgroundMain)
dragUI(AsuraX.BackgroundImage)
dragUI(AsuraX.LoginGui)


