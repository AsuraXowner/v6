	print("✅ request • User-agent: Asurax/v3.7")
	--print("✅ setfpscap • 67fps @60 • 60fps @0")
	print("✅ setfpscap • " .. resultString)
	
	print("\n")

	print("Unc Summary")
	if Configs.Settings.makefolder==true then
		print("✅ Tested with a 100% success rate (97 out of 97)")
		print("⛔ 0 tests failed")
		print("⚠️ 0 globals are missing aliases")
	elseif Configs.Settings.makefolder==false then
		print("✅ Tested with a 99% success rate (95 out of 95)")
		print("⛔ 1 tests failed")
		print("⚠️ 0 globals are missing aliases")
		end
		return resultString
		end)
end
