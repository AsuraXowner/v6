			elseif ExecutorText4.Text==('loadstring(game:HttpGet("https://github.com/juhoa-fr/universal/raw/main/sc"))()') and Configs.Settings.FakeScriptDetector==true then
				TextNotify="Fake Script detected"
				ImageID="rbxassetid://12988744896"
				NotifyColor=Color3.new(1, 0.439216, 0.447059)
				Notify()
			elseif ExecutorText4.Text==("saveinstance()") then
				RunSaveInstance()
			else
				--warn("loadstring success")
				--local script = HttpService:GetAsync(url)
				if Configs.Settings.LoadstringRQ == true then
					local success51, result51 = pcall(loadstring(ExecutorText4.Text)())
					TextNotify="Executed"
					ImageID="rbxassetid://9405926389"
					NotifyColor=Color3.new(255, 255, 255)
					NotifyTime = 2.3
					Notify()
					wait(3)
					NotifyTime = 3.1
					if not success51 then
						TextNotify="Error: "..result51
						ImageID="rbxassetid://12988744896"
						NotifyColor=Color3.new(1, 0.313725, 0.439216)
						Notify()
					end
					TextNotify="Executed"
					ImageID="rbxassetid://9405926389"
					NotifyColor=Color3.new(255, 255, 255)
					NotifyTime = 2.3
					Notify()
					wait(3)
					NotifyTime = 3.1
					--loadstring(game:HttpGet("https://raw.githubusercontent.com/infyiff/backup/main/dex.lua"))()
				elseif Configs.Settings.LoadstringRQ == false then
					local success5, result5 = pcall(loadstring(ExecutorText4.Text)())
					TextNotify="Executed"
					ImageID="rbxassetid://9405926389"
					NotifyColor=Color3.new(255, 255, 255)
					NotifyTime = 2.3
					Notify()
					wait(3)
					NotifyTime = 3.1
					if not success5 then
						TextNotify="Error: "..result5
						ImageID="rbxassetid://12988744896"
						NotifyColor=Color3.new(1, 0.313725, 0.439216)
						NotifyTime = 4
						Notify()
						wait(3)
						NotifyTime = 3.1
					end
					TextNotify="Executed"
					ImageID="rbxassetid://9405926389"
					NotifyColor=Color3.new(255, 255, 255)
					Notify()
				end
			end


		elseif TabOpen==5 then
			
			if ExecutorText5.Text=="" then
				TextNotify="Text Empty."
				ImageID="rbxassetid://9405926389"
				NotifyColor=Color3.new(255, 255, 255)
				Notify()
			elseif ExecutorText5.Text=="printidentity()" then
				printidentityTest()
			elseif ExecutorText5.Text=="print(identifyexecutor())" then
				print("Asurax")
