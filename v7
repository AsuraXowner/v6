local executor = (idenityexecutor and idenityexecutor() or getexecutename and getexecutename() or 'Unknown')
if hookmetamethod and httpServiceRun == nil and executor:lower():find('krampus') == nil then 
	if executor=="krampus" then
		ExecutorName="Krampus"
		--warn("krampus user!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!")
	elseif hookmetamethod and httpServiceRun == nil and executor:lower():find('Solara') == nil then
		--warn("solara its meh")
		ExecutorName="Solara"
	elseif hookmetamethod and httpServiceRun == nil and executor:lower():find('arceus') == nil then
		--warn("arceus its best")
		ExecutorName="Arceusx"
	elseif hookmetamethod and httpServiceRun == nil and executor:lower():find('Codex') == nil then
		--warn("codex its ok")
		ExecutorName="Codex"
	elseif hookmetamethod and httpServiceRun == nil and executor:lower():find('arceusx') == nil then
		--warn("arceus its best")
		ExecutorName="Arceusx"
	else
		ExecutorName="Unknown"
	end
end

