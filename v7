if NoPass==true then
	AsuraX.MainFrame.Visible = true
	AsuraX.LoginGui.Visible = false
	if Configs.ExecutorSettings.AutoInject==true then
		if HttpsGameGet==true then
			Injecting=true
			TextNotify="Attaching."
			ImageID="rbxassetid://9405926389"
			NotifyColor=Color3.new(255, 255, 255)
			Notify()
			wait(1.7)
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, false)
			wait(1.5)
			StarterGui:SetCoreGuiEnabled(Enum.CoreGuiType.All, true)
			Injecting=false
			Attached=true
		elseif HttpsGameGet==false then
			TextNotify="Cannot inject in roblox studio"
			ImageID="rbxassetid://12988744896"
			NotifyColor=Color3.new(1, 0.584314, 0.223529)
			Notify()
		end

	elseif Configs.ExecutorSettings.AutoInject==false then

	end
end
--version uwu

--framerate
local function setFrameRate(fps)
	if fps > 0 then
		game:GetService("RunService").RenderStepped:Connect(function(step)
			if step < 1/fps then
				wait(1/fps - step)
			end
		end)
	end
end

--function unc test
function printidentityTest()
	print("Current identity is "..ExecutorLevel)
end
function RunSaveInstance()
	TextNotify="Saving..."
	ImageID="rbxassetid://2790552399"
	NotifyColor=Color3.new(0.580392, 1, 0.615686)
	Notify()
local Params = {
 RepoURL = "https://raw.githubusercontent.com/luau/SynSaveInstance/main/",
 SSI = "saveinstance",
}
local synsaveinstance = loadstring(game:HttpGet(Params.RepoURL .. Params.SSI .. ".luau", true), Params.SSI)()
local Options = {} -- Documentation here https://luau.github.io/UniversalSynSaveInstance/api/SynSaveInstance
synsaveinstance(Options)
end



function UncTest()
	function test(name, options, func)
		local result = func()
	end

	local currentFpsCap = 0
	function setfpscap(fps)
		currentFpsCap = fps
	end

	-- Main function
	test("setfpscap", {}, function()
		local renderStepped = game:GetService("RunService").RenderStepped

		local function step()
			renderStepped:Wait()
			local sum = 0
			for _ = 1, 5 do
				sum += 1 / renderStepped:Wait()
			end
			return math.round(sum / 5)
		end

		-- Set FPS cap to 60
		setfpscap(60)
		local step60 = step()

		-- Remove FPS cap (set to unlimited)
		setfpscap(0)
		local step0 = step()

		-- Format and print the result
		local resultString = tostring(step60) .. "fps @60 • " .. tostring(step0) .. "fps @0"
	print("\n")

	print("UNC Environment Check")

	print("✅ - Pass, ⛔ - Fail, ⏺️ - No test, ⚠️ - Missing aliases\n")

	print("✅ cache.invalidate")
	print("✅ cache.iscached")
	print("✅ cache.replace")
	print("✅ cloneref")
	print("✅ compareinstances")
	print("✅ checkcaller")
	print("✅ clonefunction")
	print("⏺️ getcallingscript")
	print("✅ getscriptclosure")
	print("✅ hookfunction")
	print("✅ iscclosure")
	print("✅ islclosure")
	print("✅ isexecutorclosure")
	print("✅ loadstring")
	print("✅ newcclosure")
	print("⏺️ rconsoleclear")
	print("⏺️ rconsolecreate")
	print("⏺️ rconsoledestroy")
	print("⏺️ rconsoleinput")
	print("⏺️ rconsoleprint")
	print("⏺️ rconsolesettitle")
	print("✅ crypt.base64encode")
	print("✅ crypt.base64decode")
	print("✅ crypt.encrypt")
	print("✅ crypt.decrypt")
	print("✅ crypt.generatebytes")
	print("✅ crypt.generatekey")
	print("✅ crypt.hash")
	print("✅ debug.getconstant")
	print("✅ debug.getconstants")
	print("✅ debug.getinfo")
