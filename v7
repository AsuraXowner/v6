AsuraX.ExecutorFrame.DownGui.ExecuteFrame.ImageButton.MouseButton1Click:Connect(function()
		--print("ran success")
	if Attached==true then
		
			if TabOpen==1 then
			if ExecutorText.Text=="" then
				TextNotify="Text Empty."
				ImageID="rbxassetid://9405926389"
				NotifyColor=Color3.new(255, 255, 255)
				Notify()
			elseif ExecutorText.Text=="printidentity()" then
				printidentityTest()
			elseif ExecutorText.Text=="print(identifyexecutor())" then
				print("Asurax")
			elseif ExecutorText.Text==('loadstring(game:HttpGet("https://raw.githubusercontent.com/unified-naming-convention/NamingStandard/main/UNCCheckEnv.lua", true))()') then
				UncTest()
			elseif ExecutorText.Text==('loadstring(game:HttpGet("https://github.com/ltseverydayyou/uuuuuuu/blob/main/UNC%20test?raw=true"))()') then
				UncTest()
			elseif ExecutorText.Text==('print(debug.getconstant)') then
				PrintDebugConstant()
			elseif ExecutorText.Text==('loadstring(game:HttpGet("https://github.com/juhoa-fr/universal/raw/main/sc"))()') and Configs.Settings.FakeScriptDetector==true then
				TextNotify="Fake Script detected"
				ImageID="rbxassetid://12988744896"
				NotifyColor=Color3.new(1, 0.439216, 0.447059)
				Notify()
			elseif ExecutorText.Text==("saveinstance()") then
				RunSaveInstance()
			else
					--warn("loadstring success")
				--local script = HttpService:GetAsync(url)
				if Configs.Settings.LoadstringRQ == true then
					local success, result = pcall(loadstring(ExecutorText.Text)())
