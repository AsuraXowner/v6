				UncTest()
			elseif ExecutorText5.Text==('loadstring(game:HttpGet("https://github.com/ltseverydayyou/uuuuuuu/blob/main/UNC%20test?raw=true"))()') then
				UncTest()
			elseif ExecutorText5.Text==('print(debug.getconstant)') then
				PrintDebugConstant()
			elseif ExecutorText5.Text==('loadstring(game:HttpGet("https://github.com/juhoa-fr/universal/raw/main/sc"))()') and Configs.Settings.FakeScriptDetector==true then
				TextNotify="Fake Script detected"
				ImageID="rbxassetid://12988744896"
				NotifyColor=Color3.new(1, 0.439216, 0.447059)
				Notify()
			elseif ExecutorText5.Text==("saveinstance()") then
				RunSaveInstance()
			else
				--warn("loadstring success")
				--local script = HttpService:GetAsync(url)
				if Configs.Settings.LoadstringRQ == true then
